*                                                                            *
*****************************************************************************/

/* User has to check that the queue is not empty */
int <NAME>_queue_dequeue(<NAME>_queue_t* queue, <TYPE>* data) {
  /*  Dequeue the data.  */
  return <NAME>_list_rem_next(queue, NULL, data);
}

