*                                                                            *
*****************************************************************************/

int <NAME>_queue_enqueue(<NAME>_queue_t *queue, <TYPE> data) {
  /*  Enqueue the data.  */
  return <NAME>_list_ins_next(queue, <NAME>_list_tail(queue), data);
}

